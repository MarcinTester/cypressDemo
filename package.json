{
  "name": "uiautomatedtests",
  "version": "1.0.0",
  "description": "Automated test for https://www.saucedemo.com/",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "uiTests": "npx cypress run --spec 'cypress/integration/ui/*' --headed",
    "uiTests-chrome": "npx cypress run --browser chrome --spec 'cypress/integration/ui/*'",
    "uiTests-firefox": "npx cypress run --browser firefox --spec 'cypress/integration/ui/*'",
    "uiTests-edge": "npx cypress run --browser edge --spec 'cypress/integration/ui/*'",
    "CheckoutTest": "npx cypress run --spec 'cypress/integration/ui/CheckoutTest.js'",
    "LoginTests": "npx cypress run --spec 'cypress/integration/ui/LoginTests.js'",
    "NavigationTests": "npx cypress run --spec 'cypress/integration/ui/NavigationTests.js'"
  },
  "author": "Marcin ≈Åykowski",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.17.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-split": "^1.24.0",
    "mocha": "^10.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
